#include <iostream>
 
using namespace std;
 
void polowa(string slowo);
 
void polowa(string slowo)
{
  int t;
  string napis;
 
  cin>>t;
 
  for(int i=0; i<t; i++)
  {
        cin>>napis;
        int dlugosc=napis.length();
 
        for(int j=0; j<dlugosc/2; j++)
        {
            cout<<napis[j];
        }
        cout<<endl;
  }
}
 
int main()
{
    string slowo;
    polowa(slowo);
 
    return 0;
}
